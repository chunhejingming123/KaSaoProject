<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_90" />
            <gradient android:angle="0" android:endColor="@color/color_b91b6c" android:startColor="@color/color_ee2e3b" android:type="linear" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/dimen_90" />

            <gradient android:angle="0" android:endColor="@color/color_ee2e3b" android:startColor="@color/color_b91b6c" android:type="linear" />
        </shape>
    </item>
</selector>